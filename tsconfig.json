{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowArbitraryExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@pages/*": [
        "./pages/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@redux/*": [
        "./redux/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@data/*": [
        "./data/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@common/*": [
        "./components/common/*"
      ],
      "@auth/*": [
        "./components/auth/*"
      ],
      "@services/*": [
        "./services/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
